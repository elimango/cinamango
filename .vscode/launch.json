{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cinema 4D: Remote Debugger Attacher",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "127.0.0.1",
                "port": 3000
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ]
        }
    ]
}